{
  "name": "silent-code-mentor",
  "displayName": "Silent Code Mentor",
  "description": "Silently optimizes and enhances web developers' code in real time.",
  "version": "0.1.2",
  "publisher": "sbs",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:html",
    "onLanguage:css",
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/samibs/your-repo.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "silentCodeMentor.helloWorld",
        "title": "Silent Code Mentor: Hello World"
      }
    ]
  },
  "dependencies": {
    "@babel/parser": "^7.26.3",
    "escodegen": "^2.1.0",
    "typescript": "^5.7.2"
  }
}
